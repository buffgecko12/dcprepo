CREATE OR REPLACE FUNCTION $APP_NAME$Views.SP_DCPDeleteUserGroup (
    pGroupUserId INTEGER
)
RETURNS VOID AS $$
BEGIN
	-- Delete group info
	DELETE FROM $APP_NAME$.User_Group WHERE GroupUserId = pGroupUserId;
	
	-- Delete group user
	PERFORM $APP_NAME$Views.SP_DCPDeleteUser(pGroupUserId);
	
END;
$$ LANGUAGE 'plpgsql';